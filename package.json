{
  "name": "peaknewsdaily",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "e2e": "playwright test",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "seed:dummy": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.dummy.ts",
    "seed:dummy:append": "SEED_APPEND=true ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.dummy.ts",
    "prune:broken": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/prune.broken.ts",
    "fix:media": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/fix.media.ts",
    "update:financial": "node scripts/update-financial-data.js",
    "update:prices": "node scripts/update-prices.js",
    "seed:market": "node scripts/seed-market-data.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@giscus/react": "^3.0.0",
    "@prisma/client": "^5.17.0",
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.542.0",
    "next": "^14.2.32",
    "next-auth": "^5.0.0-beta.20",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "resend": "^3.4.0",
    "rss-parser": "^3.13.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "playwright": "^1.46.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
